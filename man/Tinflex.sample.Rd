\name{Tinflex.sample}
\alias{Tinflex.sample}

\title{
  Draw Random Sample from Tinflex Generator Object 
}

\description{
  Draw a random sample from a generator object of class \code{"Tinflex"}.
}

\usage{
Tinflex.sample(gen, n=1)
}

\arguments{
  \item{gen}{an object of class \code{"Tinflex"}.}
  \item{n}{sample size. (integer)}
}

